        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="23dp"
        android:layout_marginEnd="44dp"
        android:shadowColor="#4CAF50"
