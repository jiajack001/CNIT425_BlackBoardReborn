<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#272525">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#1D1C1C"
        android:text="Pick a date"
        android:textColor="#FFC107"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="146dp"
        tools:layout_editor_absoluteY="109dp" />

    <Button
        android:id="@+id/signUpButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Next"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="627dp" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#1D1C1C"
        android:text="Vaccination Sign Up"
        android:textColor="#FFC107"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="99dp"
        tools:layout_editor_absoluteY="29dp" />

    <CalendarView
        android:id="@+id/signUpCalendarView"
        android:layout_width="344dp"
        android:layout_height="331dp"
        android:background="#705858"
        android:focusedMonthDateColor="#AEA0A0"
        android:weekDayTextAppearance="@style/TextAppearance.AppCompat.Large"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="186dp" />

</androidx.constraintlayout.widget.ConstraintLayout>